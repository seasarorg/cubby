	<!--component class="org.seasar.cubby.controller.impl.RoutingsDispatcherImpl" /-->
	<!--component class="org.seasar.cubby.controller.impl.CubbyConfigurationImpl" /-->
	<!--component class="org.seasar.cubby.controller.impl.RequestParameterBinderImpl"/-->
	<!--component class="org.seasar.cubby.controller.impl.FormWrapperFactoryImpl"/-->

	<!-- handlers -->
	<component name="actionHandlers" class="java.util.ArrayList">
		<initMethod name="add">
			<arg><component class="org.seasar.cubby.handler.impl.ExceptionActionHandler" /></arg>
		</initMethod>
		<initMethod name="add">
			<arg><component class="org.seasar.cubby.handler.impl.InitializeActionHandler" /></arg>
		</initMethod>
		<initMethod name="add">
			<arg><component class="org.seasar.cubby.handler.impl.ValidationActionHandler" /></arg>
		</initMethod>
		<initMethod name="add">
			<arg><component class="org.seasar.cubby.handler.impl.InvocationActionHandler" /></arg>
		</initMethod>
	</component>

