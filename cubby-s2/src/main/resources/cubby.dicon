	<!-- spi -->
	<component class="org.seasar.cubby.plugins.s2.factory.S2PathResolverFactory" />
	<component class="org.seasar.cubby.plugins.s2.factory.S2RequestParserFactory" />
	<component class="org.seasar.cubby.plugins.s2.factory.S2ConverterFactory" />
	<component class="org.seasar.cubby.plugins.s2.factory.S2ActionHandlerChainFactory" />

	<!-- routing -->
	<!--component class="org.seasar.cubby.routing.impl.S2PathResolverImpl" /-->
	<!--component class="org.seasar.cubby.routing.impl.RouterImpl" /-->

