<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container//EN"
	"http://www.seasar.org/dtd/components.dtd">
<components>
	<include path="cubby.dicon"/>

	<component class="org.seasar.cubby.routing.impl.PathResolverImpl">
		<property name="uriEncoding">"UTF-8"</property>
		<initMethod name="add">
			<arg>"/routing/test"</arg>
			<arg>@org.seasar.cubby.action.MockAction@class</arg>
			<arg>"dummy1"</arg>
		</initMethod>
		<initMethod name="add">
			<arg>"/routing/test/{value1,[0-9]+}/{value2}"</arg>
			<arg>@org.seasar.cubby.action.MockAction@class</arg>
			<arg>"dummy2"</arg>
		</initMethod>
	</component>

	<component class="org.seasar.cubby.routing.impl.RouterImpl" />

	<component class="org.seasar.cubby.action.MockAction" />

</components>