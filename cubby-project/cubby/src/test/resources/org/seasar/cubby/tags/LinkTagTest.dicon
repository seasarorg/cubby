<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container//EN"
	"http://www.seasar.org/dtd/components.dtd">
<components>
	<include path="dxo.dicon"/>
	<include path="cubby.dicon"/>

	<component class="org.seasar.framework.convention.impl.NamingConventionImpl">
		<initMethod name="addRootPackageName">
			<arg>"org.seasar.cubby.tags"</arg>
		</initMethod>
	</component>

	<component class="org.seasar.cubby.routing.impl.PathResolverImpl">
		<property name="uriEncoding">"UTF-8"</property>
		<initMethod name="add">
			<arg>"/mockFormTagTest/foo"</arg>
			<arg>@org.seasar.cubby.tags.MockFormTagTestAction@class</arg>
			<arg>"foo"</arg>
		</initMethod>
		<initMethod name="add">
			<arg>"/mockFormTagTest/bar/{id}"</arg>
			<arg>@org.seasar.cubby.tags.MockFormTagTestAction@class</arg>
			<arg>"bar"</arg>
		</initMethod>
	</component>

	<component class="org.seasar.cubby.routing.impl.RouterImpl" />

</components>
